<section class="modal" id="create-supplier" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <section class="modal-dialog modal-lg">
	<section class="modal-content">
	  <section class="modal-header">
		<button type="button" class="btn btn-default pull-right" data-dismiss="modal" aria-label="Close">Close</button>
		<h4 class="modal-title" id="productMenuName">Create Supplier</h4>
	  </section>
	  <section class="modal-body">
	  	<section class="container-fluid">
	  		<section class="row">
	  			<section class="col-md-12">
					  <section class="form-group">
						<label for="new-supplier-name" class="control-label">Name</label>
						<input type="text" id="new-supplier-name" class="form-control input-lg">
					  </section>
					  <section class="form-group">
						<label for="new-supplier-telephone" class="control-label">Telephone</label>
						<input type="text" id="new-supplier-telephone" class="form-control input-lg">
					  </section>
				      <section class="form-group">
						<label for="new-supplier-email" class="control-label">Email</label>
						<input type="text" id="new-supplier-email" class="form-control input-lg">
					  </section>  
					  <section class="form-group">
						<label for="new-supplier-website" class="control-label">Website</label>
						<input type="text" id="new-supplier-website" class="form-control input-lg">
					  </section>
					   <section class="form-group">
						<label for="new-supplier-comments" class="control-label">Comments</label>
						<textarea id="new-supplier-comments" class="form-control input-lg"></textarea>
					  </section>
				</section>
  			</section>
  		</section>
  	  </section>
	  <section class="modal-footer">
