<section class="modal" id="supplierInfo" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <section class="modal-dialog modal-lg">
	<section class="modal-content">
	  <section class="modal-header">
		<button type="button" class="btn btn-default pull-right" data-dismiss="modal" aria-label="Close">Close</button>
		<h4 class="modal-title" id="productMenuName">Supplier Edit</h4>
	  </section>
	  <section class="modal-body">
	  	<section class="container-fluid">
	  		<section class="row">
	  			<section class="col-md-12">
					  <section class="form-group">
						<label for="supplier-name" class="control-label">Name</label>
						<input type="text" id="supplier-name" class="form-control input-lg">
					  </section>
					  <section class="form-group">
						<label for="supplier-telephone" class="control-label">Telephone</label>
						<input type="text" id="supplier-telephone" class="form-control input-lg">
					  </section>
				      <section class="form-group">
						<label for="supplier-email" class="control-label">Email</label>
						<input type="text" id="supplier-email" class="form-control input-lg">
					  </section>  
					  <section class="form-group">
						<label for="supplier-website" class="control-label">Website</label>
						<input type="text" id="supplier-website" class="form-control input-lg">
					  </section>
					   <section class="form-group">
						<label for="supplier-comments" class="control-label">Comments</label>
						<textarea id="supplier-comments" class="form-control input-lg"></textarea>
					  </section>
				</section>
  			</section>
  		</section>
  	  </section>
	  <section class="modal-footer">
	  	<button type="button" class="btn btn-danger btn-lg pull-left" id="delete-supplier">Delete Supplier</button>
		<button type="button" class="btn btn-success btn-lg" id="update-supplier">Update Supplier</button>
	  </section>
  </section>
 </section>
 </section>
